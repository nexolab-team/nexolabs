<svg width="800" height="400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="alertGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ff0000;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#ff00ff;stop-opacity:0.8" />
    </linearGradient>
  </defs>
  <rect width="800" height="400" fill="#1a1a1a"/>
  
  <!-- Alert box -->
  <rect x="150" y="100" width="500" height="200" fill="#fff" stroke="#ccc" stroke-width="2" rx="8"/>
  <rect x="150" y="100" width="500" height="40" fill="#f0f0f0" stroke="#ccc" stroke-width="2" rx="8"/>
  
  <!-- Alert title -->
  <text x="180" y="128" font-family="Arial" font-size="16" fill="#333">target.ctf.com says</text>
  
  <!-- Alert message -->
  <text x="400" y="180" font-family="monospace" font-size="20" fill="#ff0000" text-anchor="middle" font-weight="bold">XSS Vulnerability Confirmed!</text>
  <text x="400" y="210" font-family="monospace" font-size="16" fill="#00ff00" text-anchor="middle">document.domain = target.ctf.com</text>
  
  <!-- OK button -->
  <rect x="320" y="240" width="160" height="40" fill="#007bff" rx="4"/>
  <text x="400" y="267" font-family="Arial" font-size="16" fill="#fff" text-anchor="middle">OK</text>
  
  <!-- Warning icon -->
  <circle cx="200" cy="170" r="30" fill="none" stroke="#ff0000" stroke-width="3"/>
  <text x="200" y="185" font-family="Arial" font-size="40" fill="#ff0000" text-anchor="middle" font-weight="bold">!</text>
</svg>
